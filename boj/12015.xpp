int n;
cin >> n;
vector<int> a(n);
for(int i=0; i<n; i++) {
  cin >> a[i];
}
static constexpr auto big = numeric_limits<int>::max();
vector<int> lis(n, big);
for(int i=0; i<n; i++) {
  *lower_bound(begin(lis), end(lis), a[i]) = a[i];
}
cout << lower_bound(begin(lis), end(lis), big) - begin(lis) << nl;
