static constexpr int n = 8;
static constexpr auto edges = {
  pair(0, 1), pair(0, 2), pair(1, 2),
  pair(1, 3), pair(2, 3),
  pair(2, 4), pair(3, 4),
  pair(3, 5), pair(4, 5),
  pair(4, 6), pair(5, 7),
  pair(6, 7),
};
using Int = mod_int<1'000'000'007>;
square_matrix<Int, n> mat;
for(auto [a, b] : edges) {
  mat[a][b] = mat[b][a] = 1;
}

int d;
cin >> d;
square_matrix<Int, n> res(1);
for(; d; d/=2) {
  if(d&1) res *= mat;
  mat *= mat;
}
cout << res[0][0] << nl;
